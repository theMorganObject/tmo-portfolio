{
  "extends": ["next/core-web-vitals", "plugin:prettier/recommended"],
  "rules": {
    "quotes": ["error", "single", { "avoidEscape": true }],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "jsxSingleQuote": true,
        "semi": true,
        "endOfLine": "lf"
      }
    ]
  }
}
